{
    "$schema": "http://json-schema.org/schema#",
    "title": "Schema for client-inquiry-response",
    "id": "http://infarmbureau.com/schemas/client-inquiry-response.json",
    "data" : {
        "type" : "array",
        "items" : {
            "clients" : {
                "type" : "array",
                "items" : {
                    "id" : {"type" : "string" },
                    "name" : {"type" : "string" },
                    "firstName" : {"type" : "string" },
                    "lastName" : {"type" : "string"},
                    "dateOfBirth" : { "type" :  "string", "format" : "date" },
                    "taxId" : { "type" : "string" },
                    "rootId" : { "type" : "string" },
                    "type" : { "type" : "string" },
                    "typeCode" : { "type" : "string" },
                    "childId" : { "type" : "stirng" }
                },
                "required" : [
                    "id", 
                    "name",
                    "firstName",
                    "lastName",
                    "dateOfBirth",
                    "taxId",
                    "rootId",
                    "type",
                    "typeCode",
                    "childId"
                ]
            }
        },
        "required" : [
            "clients"
        ]
    },
    "required": [
        "data"
    ]
}